{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": [1, 4],
        "linebreak-style": 0,
        "comma-dangle": ["error", "never"],
        "no-unneeded-ternary": 0,
        "max-len" : "off",
        "eqeqeq" : "off",
        "no-console" : "off"
    }
}
